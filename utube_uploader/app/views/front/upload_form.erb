<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>Upload</title>
    <script type="text/javascript" src="http://www.google.com/jsapi"></script>
<!--    <script src="3rdparty/timeline_2.3.0/timeline_js/timeline-api.js" type="text/javascript"></script>-->
    <script src="3rdparty/jquery/jquery-1.4.2.min.js" type="text/javascript"></script>
    <script src="3rdparty/jquery/jquery.cookie.js" type="text/javascript"></script>
    <script src="lib/cfg.js" type="text/javascript"></script>
    <script src="lib/main.js" type="text/javascript"></script>
  </head>
  <body>
    <div id="cont">
      <h3>Upload a movie:</h3><br />
      <div id="uploaderForm">
        <form action="<%= @action %>?nexturl=http://defaulter.dyndns.hu:3000/ready" method="post" enctype="multipart/form-data">
        <input id="file" type="file" name="file"/>
        <input type="hidden" name="token" value="<%= @responsetoken %>"/>
        <input type="submit" value="go" />
        </form>
      </div>
    </div>
  </body>
</html>

