.dropmenudiv{
position:absolute;
padding-bottom:5px;
/*margin-left:-10px;*/
padding-top: 7px;
/*margin-top:8px;*/
line-height:18px;
z-index:100;
opacity: .9;
filter: alpha(opacity=90);
background-color: #f5f5f5;
border-left:1px solid #333332;
border-bottom:1px solid #c7c7c7;
border-right:1px solid #c7c7c7;
}

.dropmenudiv a {
width: 100%;
display: block;
text-indent: 3px;
padding: 0px 0px 0px 5px;
text-decoration: none;
font-weight: normal;
text-align: left;
color: #727272;
background:url(gfx/button.jpg) no-repeat left;
}

.dropmenudiv a:hover {
display: block;
width: 100%;
color: #333332; 

