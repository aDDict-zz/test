body{
	padding:0;
	margin:0;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:10px;
}
input, select{font-size:10px;width:180px;font-family:Verdana, Arial, Helvetica, sans-serif;}
textarea{font-size:11px;width:180px;font-family:Verdana, Arial, Helvetica, sans-serif;}
.button{width:auto;}
div#content{
	width:912px;
	margin:auto;	
}
div#header{
	width:100%;
	height:85px;
	margin:auto;
	background-image:url(../../i/b.gif);
	background-repeat:repeat-x;
}
div#row{
	width:100%;
	height:19px;
	padding-top:3px;
	background-color:#39567D;
	color:#FFFFFF;
	text-align:right;
}
a:link{color:#000000;text-decoration:none}
a:visited{color:#000000;text-decoration:none}
a:hover{color:#000000;text-decoration:underline}
a:active{color:#000000;text-decoration:none}

a.top_menu_link,a.top_menu_link:link,a.top_menu_link:visited,a.top_menu_link:active{
	display:block;
	width:100%;
	padding:3px 0px 3px 0px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:10px;
	font-weight:bold;
	color:#000000;
	text-decoration:none;
}
a.top_menu_link:hover{
	text-decoration:underline;
	color:#FFFFFF;
}

a.top_menu_link_sel,a.top_menu_link_sel:link,a.top_menu_link_sel:visited,a.top_menu_link_sel:active{
	display:block;
	width:100%;
	padding:3px 0px 3px 0px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:10px;
	font-weight:bold;
	color:#FFFFFF;
	text-decoration:none;
}
a.top_menu_link_sel:hover{
	text-decoration:underline;
	color:#FFFFFF;
}

.top_menu{
	background-image:url(../../i/top_menu.gif);
	background-repeat:repeat-x;
	height:22px;
	border-right:1px solid #FFFFFF;
	border-bottom:1px solid #FFFFFF;	
	padding:0px 3px 0px 3px;
	text-align:center;
	white-space:nowrap;
}

.top_menu_sel{
	background-image:url(../../i/top_menu_sel.gif);
	background-repeat:repeat-x;
	height:23px;
	border-right:1px solid #FFFFFF;
	padding:0px 3px 0px 3px;
	text-align:center;
	white-space:nowrap;
}

.sub_menu{
	background-image:url(../../i/sub_menu.gif);
	background-repeat:repeat-x;
	width:100%;
	height:20px;
	border-right:1px solid #FFFFFF;	
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:9px;
	font-weight:bold;
	color:#FFFFFF;
	white-space:nowrap;	
}
.sub_menu_link:link{
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:9px;
	font-weight:bold;
	color:#FFFFFF;
	text-decoration:none;
}
.sub_menu_link:visited{
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:9px;
	font-weight:bold;
	color:#FFFFFF;
	text-decoration:none;
}

.sub_menu_link:hover{
	text-decoration:underline;
}
.sub_menu_link:active{
	color:#FFFFFF;
}
.page_subtitle{
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:14px;
	font-weight:bold;
	color:#B2B2B2;
	padding:15px 0 0 0;
}
.page_title{
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:16px;
	font-weight:bold;
	color:#B2B2B2;
	padding:15px 0px 15px 0px;
}
.box{
	border:1px solid #CCCCCC;
	margin:0 0  8px 0;
}
.box h2{
	cursor:move;
	margin:0px;
	font-size:12px;
	padding:3px;
	background:#EFF5FF;
	border-bottom:1px solid #CCCCCC;	
}
.box div img{
	cursor:pointer;
}
.box .edit{
	background:#EFF5FF;
	display:none;
}
.box .content {
	padding:4px;
}
.box .links h2{
	font-weight:normal;
	font-size:11px;
	border:0;
	display:none;	
}
.box .links .newlink{
	display:none;
}
.box .links .searchlinks{
	display:none;
}
.htmllink, .rss{display:none;}
.box .link_list{margin:8px 0 8px 0;display:none;}
.box .link_list ul, .box .link_list p{	
	list-style-type:none;
	margin:0;
	padding:0;
}
.box .link_list ul li{float:left;display:block}
